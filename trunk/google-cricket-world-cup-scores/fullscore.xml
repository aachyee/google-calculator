    <label height="15" name="ba8" width="145" x="6" y="203" font="Arial" size="9" />
    <label height="15" name="ba9" width="145" x="6" y="219" font="Arial" size="9" />
    <label height="15" name="ba10" width="145" x="6" y="235" font="Arial" size="9" />
    <label height="15" name="ba11" width="145" x="6" y="251" font="Arial" size="9" />
    
    <!-- howout -->
    <label height="15" name="st1" width="180" x="151" y="91" font="Arial" size="9" />
    <label height="15" name="st2" width="180" x="151" y="107" font="Arial" size="9" />
    <label height="15" name="st3" width="180" x="151" y="123" font="Arial" size="9" />
    <label height="15" name="st4" width="180" x="151" y="139" font="Arial" size="9" />
    <label height="15" name="st5" width="180" x="151" y="155" font="Arial" size="9" />
    <label height="15" name="st6" width="180" x="151" y="171" font="Arial" size="9" />
    <label height="15" name="st7" width="180" x="151" y="187" font="Arial" size="9" />
    <label height="15" name="st8" width="180" x="151" y="203" font="Arial" size="9" />
    <label height="15" name="st9" width="180" x="151" y="219" font="Arial" size="9" />
    <label height="15" name="st10" width="180" x="151" y="235" font="Arial" size="9" />
    <label height="15" name="st11" width="180" x="151" y="251" font="Arial" size="9" />
    
    <!-- scores -->
    <label height="15" name="s1" width="60" x="330" y="91" font="Arial" size="9" bold="false" align="right" />
    <label height="15" name="s2" width="60" x="330" y="107" font="Arial" size="9" bold="false" align="right" />
    <label height="15" name="s3" width="60" x="330" y="123" font="Arial" size="9" bold="false" align="right" />
    <label height="15" name="s4" width="60" x="330" y="139" font="Arial" size="9" bold="false" align="right" />
    <label height="15" name="s5" width="60" x="330" y="155" font="Arial" size="9" bold="false" align="right" />
    <label height="15" name="s6" width="60" x="330" y="171" font="Arial" size="9" bold="false" align="right" />
    <label height="15" name="s7" width="60" x="330" y="187" font="Arial" size="9" bold="false" align="right" />
    <label height="15" name="s8" width="60" x="330" y="203" font="Arial" size="9" bold="false" align="right" />
    <label height="15" name="s9" width="60" x="330" y="219" font="Arial" size="9" bold="false" align="right" />
