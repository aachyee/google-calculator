    <label height="15" name="s11" width="60" x="330" y="251" font="Arial" size="9" bold="false" align="right" />

    <!-- Fall of wickets -->
    <label height="16" name="fow" width="125" x="5" y="275" bold="true"
      color="#444444" font="Arial" size="9">&strFow;</label>
    <label height="58" name="fowtext" width="390" x="5" y="290"
      font="Arial" size="8" wordwrap="true"/>

    <!-- bowler names -->
    <label height="15" name="bo1" width="145" x="6" y="371" font="Arial" size="9" />
    <label height="15" name="bo2" width="145" x="6" y="387" font="Arial" size="9" />
    <label height="15" name="bo3" width="145" x="6" y="403" font="Arial" size="9" />
    <label height="15" name="bo4" width="145" x="6" y="419" font="Arial" size="9" />
    <label height="15" name="bo5" width="145" x="6" y="435" font="Arial" size="9" />
    <label height="15" name="bo6" width="145" x="6" y="451" font="Arial" size="9" />
    <label height="15" name="bo7" width="145" x="6" y="467" font="Arial" size="9" />

    <!-- bowler stats -->
    <label height="15" name="bs1" width="145" x="151" y="371" font="Arial" size="9" />
    <label height="15" name="bs2" width="145" x="151" y="387" font="Arial" size="9" />
    <label height="15" name="bs3" width="145" x="151" y="403" font="Arial" size="9" />
    <label height="15" name="bs4" width="145" x="151" y="419" font="Arial" size="9" />
    <label height="15" name="bs5" width="145" x="151" y="435" font="Arial" size="9" />
    <label height="15" name="bs6" width="145" x="151" y="451" font="Arial" size="9" />
    <label height="15" name="bs7" width="145" x="151" y="467" font="Arial" size="9" />
    
    <!-- status/error layer -->
    <div name="status" width="100%" height="100%" background="#FFFFFF">
      <label height="100%" name="status_text" width="100%" align="center"
        bold="true" color="#646464" font="Arial" size="9" valign="middle"
        wordwrap="true"/>
    </div>

    <!-- scripts -->
    <script src="fullscore.js" />
</view>
