<view height="500" resizable="false" width="400" onopen="view_onOpen()">
    <label name="crr" x="260" y="20" width="135" height="16" 
      font="Arial" bold="true" size="9" />
    <label name="rrr" x="260" y="40" width="135" height="16"
      font="Arial" bold="true" size="9" />
    <label name="runs_text" x="0" y="4" width="85" height="25"
      font="Arial" bold="true" size="18" />
    <label name="overs_text" x="2" y="30" width="89" height="17" bold="true"
      font="Arial" size="11" />
    <label name="trgt" height="17" width="89" x="2" y="46"
      font="Arial" size="8" />
    <label height="19" name="batting" width="92" x="2" y="71" bold="true"
      color="#444444" font="Arial" size="11">&strBat;</label>
    <label height="26" name="bowling" width="89" x="2" y="350" bold="true"
      color="#444444" font="Arial" size="11">&strBowl;</label>
      
    <!-- alternating backgrounds for batsmen and bowlers -->
    <div height="15" name="d1" width="390" x="4" y="91" background="#E4EBEE"/>
    <div height="15" name="d2" width="390" x="4" y="123" background="#E4EBEE"/>
    <div height="15" name="d3" width="390" x="4" y="155" background="#E4EBEE"/>
    <div height="15" name="d4" width="390" x="4" y="187" background="#E4EBEE"/>
