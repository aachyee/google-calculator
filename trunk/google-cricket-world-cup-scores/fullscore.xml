    <div height="15" name="d6" width="390" x="4" y="251" background="#E4EBEE"/>
    <div height="15" name="d7" width="390" x="4" y="371" background="#E4EBEE"/>
    <div height="15" name="d8" width="390" x="4" y="403" background="#E4EBEE"/>
    <div height="15" name="d9" width="390" x="4" y="435" background="#E4EBEE"/>
    <div height="15" name="d10" width="390" x="4" y="467" background="#E4EBEE"/>
    
    <!-- batsmen names -->
    <label height="15" name="ba1" width="145" x="6" y="91" font="Arial" size="9" />
    <label height="15" name="ba2" width="145" x="6" y="107" font="Arial" size="9" />
    <label height="15" name="ba3" width="145" x="6" y="123" font="Arial" size="9" />
    <label height="15" name="ba4" width="145" x="6" y="139" font="Arial" size="9" />
